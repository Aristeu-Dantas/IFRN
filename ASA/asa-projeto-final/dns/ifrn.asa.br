;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.ifrn.asa.br. root.asa.br. (
			 4     		; Serial
			 604823		; Refresh
			  86423		; Retry
			2419223		; Expire
			 604823 )	; Negative Cache TTL
;
@	IN	NS	ns.ifrn.asa.br.
@	IN  MX	10 mail.ifrn.asa.br.
@	IN	A	127.0.0.1
@	IN	A	127.0.0.1
ns		A 	127.0.0.1;
mail	A 	127.0.0.1;
www		CNAME	ns
